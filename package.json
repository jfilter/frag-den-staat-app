      "no-underscore-dangle": "off",
      "arrow-body-style": "off",
      "react/jsx-wrap-multilines": "off",
      "jsx-a11y/href-no-hash": "off",
      "global-require": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    },
    "globals": {
      "fetch": false,
      "require": false,
      "__DEV__": false
    }
